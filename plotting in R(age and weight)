# Load necessary libraries
library(ggplot2)

# Create a data frame
data <- data.frame(Age = ages, Weight = weights)

# Scatter plot
ggplot(data, aes(x = Age, y = Weight)) +
  geom_point(color = "blue") +
  labs(title = "Scatter Plot of Age vs. Weight", x = "Age", y = "Weight") +
  theme_minimal()